								<sci:legalvalscript>#legalVals := &apos;Fingerprint&apos;;
if parameter(&apos;Type&apos;) ne &apos;MDLPublicKeys&apos; then
	append(#legalVals, &apos;Counts&apos;);
end if;
	
setLegalValues(#legalVals);</sci:legalvalscript>
